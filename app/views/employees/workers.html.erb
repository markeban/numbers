<h3> Would you like to add an employee? If so, add one to the form: </h3> 

<p>Here are all the employees in the company:</p>




<%= form_tag '/save' do %>

<div>
  <%= label_tag :first_name %>
  <%= text_field_tag :first_name %>
</div>

<div>
  <%= label_tag :last_name %>
  <%= text_field_tag :last_name %>
</div>

<div>
  <%= label_tag :email %>
  <%= text_field_tag :email %>
</div>

<div>
  <%= label_tag :job_title %>
  <%= text_field_tag :job_title %>
</div>

<div>
  <%= label_tag :salary %>
  <%= text_field_tag :salary %>
</div>

<div>
  <%= label_tag :phone_number %>
  <%= text_field_tag :phone_number %>
</div>

<div>
  <%= label_tag :gender %>
  <%= text_field_tag :gender %>
</div>

<% end %>
